{
  "name": "wdio-page-objects",
  "version": "0.0.4",
  "description": "Simple page object pattern realization for WebdriverIO",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "npx nyc wdio ./test/wdio.conf.js",
    "coveralls": "npx cat ./coverage/lcov.info | npx coveralls",
    "build": "npm run clean & npx tsc",
    "clean": "npx rimraf lib"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aracturat/wdio-page-objects.git"
  },
  "author": "Nikolay Dozmorov <ndozmorov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aracturat/wdio-page-objects/issues"
  },
  "homepage": "https://github.com/Aracturat/wdio-page-objects#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.10",
    "@types/webdriverio": "^4.8.6"
  },
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-mochawesome-reporter": "^1.1.0",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.9.11"
  }
}
