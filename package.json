{
  "name": "wdio-page-objects",
  "version": "0.1.3",
  "description": "Simple page object pattern realization for WebdriverIO",
  "keywords": [
    "e2e",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "page objects",
    "typescript",
    "decorators"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "npx nyc wdio ./test/wdio.conf.js",
    "coveralls": "npx cat ./coverage/lcov.info | npx coveralls",
    "build": "npm run clean & npx tsc",
    "clean": "npx rimraf lib & npx rimraf coverage & npx rimraf .nyc_output"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "lib/*.js",
      "lib/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aracturat/wdio-page-objects.git"
  },
  "author": "Nikolay Dozmorov <ndozmorov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aracturat/wdio-page-objects/issues"
  },
  "homepage": "https://github.com/Aracturat/wdio-page-objects#readme",
  "dependencies": {
    "@types/webdriverio": "^4.10.1",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "express": "^4.16.3",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "source-map-support": "^0.5.4",
    "typescript": "^2.7.2",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "^0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^5.0.0"
  }
}
